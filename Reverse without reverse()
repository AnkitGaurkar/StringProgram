package com;

import java.util.Arrays;
import java.util.Scanner;

public class Demo {
	
	public static void main(String[]args)
	{
		
		String s="I am Ankit";
		
		//String rev="";
		
		for(int i=s.length()-1;i>=0;i--)
		{
			System.out.print(s.charAt(i));
		//	rev=rev+s.charAt(i);
		}
		//		System.out.println(rev);		
		}
	}
