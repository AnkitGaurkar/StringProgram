package com;

import java.util.Arrays;
import java.util.Scanner;

public class Demo {
	
	public static void main(String[]args)
	{
		
		String s="I am Ankit";
		int count=0;
		
		for(int i=0;i<s.length();i++)
		{
			if(s.charAt(i)!=' ')
			{
				count++;
			}
		}
		System.out.println(count);
		
	}
	}
